# Variáveis de ambiente para Railway
FLASK_ENV=production
SECRET_KEY=railway_secret_key_change_in_production
PORT=5000
BLAZE_WEBSOCKET_URL=wss://play.soline.bet:5903/Game
ANALYZER_ENABLED=true
NOTIFICATIONS_ENABLED=false
DESKTOP_NOTIFICATIONS=false
SOUND_ALERTS=false

# Credenciais Pragmatic Play (configure no Railway Dashboard)
PLAYNABETS_USER=seu_email@exemplo.com
PLAYNABETS_PASS=sua_senha

# Configurações Pragmatic Play
PRAGMATIC_AUTO_START=false
PRAGMATIC_SESSION_DURATION=3600
PRAGMATIC_RECONNECT_ATTEMPTS=5

# Configurações Playwright
PLAYWRIGHT_BROWSERS=chromium
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_TIMEOUT=30000
